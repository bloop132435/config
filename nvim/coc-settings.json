{
	"diagnostic.infoSign": "\u24d8",
	"diagnostic.errorSign": "x",
	"diagnostic.hintSign": "\u203d",
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextCurrentLineOnly": false,
	"hover.target": "float",
	"codeLens.enable": true,
	"list.nextKeymap": "<C-n>",
	"list.previousKeymap": "<C-p>",
	"workspace.rootPatterns": [
		"compile_flags.txt"
	],
	"coc.preferences.formatOnSaveFiletypes": [
		"*.rs",
		"*.cpp",
		"*.c",
		"*.java",
		"*.py"
	],
	"coc.preferences.formatOnType": true,
	"suggest.languageSourcePriority": 100,
	"java.format.enabled": true,
	"java.format.comments.enabled": true,
	"Lua.telemetry.enable": true,
	"rust-analyzer.cargo.features": "all",
	"rust-analyzer.lens.enable": true,
	"rust-analyzer.hover.links.enable": true,
	"rust-analyzer.terminal.startinsert": false,
	"rust-analyzer.joinLines.joinElseIf": true,
	"sumneko-lua.enableNvimLuaDev": true,
	"zig.enabled": true,
	"zig.startUpMessage": true,
	"zig.path": "$HOME/zls/zls",
	"zig.debugLog": false,
	"pairs.enableCharacters":
	["(", "[", "{", "'", "\"", "`"],
	"rust-analyzer.lens.run.enable": false,
	"rust-analyzer.lens.debug.enable": false
}
